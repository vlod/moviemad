<div class="container-fluid">
  <div class="row-fluid">
    <%= render partial:'categories/category_list' %>

    <div class="span9">
      <div class="row-fluid">
        <div class="span9 movie_list">
          <h4>Movies for: <%=@category.name %></h4>
          <ul>
            <% @movies.each do |movie| %>
              <li id="m_<%=movie.id%>">
                <% if movie.title_md5 %>
                  <%= link_to movie.title, "#", "data-movie-id"=>"#{movie.id}", "class"=>"movie_entry" %>
                <% else %>
                  <label><%=movie.title%></label>
                <% end %>
            <% end %>
        </div>
      </div>
      <div class="row-fluid">
        <div class="span9">
          <%= paginate @movies %>
        </div>
      </div>
    </div>
  </div>
</div>